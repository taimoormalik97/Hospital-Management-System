  <%#= javascript_include_tag 'https://cdn.datatables.net/s/dt/dt-1.10.10,r-2.0.0/datatables.min.js' %> 
  <body>
    <%= render_breadcrumbs separator: ' / ', class: 'breadcrumb'%>
