<% list = Availability.where(doctor_id: params[:doctor_id], week_day: params[:date]).all %>
<%= select_tag 'appointment[availability_id]', options_for_select(list.map { |availability| ["#{availability.start_slot.strftime("%I:%M%p")} - #{availability.end_slot.strftime("%I:%M%p")}", availability.id]}), {class: "form-control"} %> 
